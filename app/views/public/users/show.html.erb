
<div class="container">
  <div class="row">
    <div class="col-12">
      <h1>My Note</h1>
      <div class="row">
        <%= @user.name %>
        <%#= image_tag @user.get_image(100,100) %>
      </div>
      <div class="row">
        <% @notes.each do |note| %>
      <div class="note_box2_6">
        <h2><%= link_to note.title,note_path(note.id) %></h2>
        <p><%= link_to note.body, note_path(note.id) %></p>
      </div>
      <% end %>


      </div>
    </div>
  </div>
</div>