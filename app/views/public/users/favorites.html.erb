<div class="container">
  <div class="row">
    <div class="col-12">
      <h1>♡Notes</h1>
      <p>いいねしたノート</p>
      <% if @favorite_notes.present? %>
        <% @favorite_notes.each do |note|%>
        <%= render '/public/shared/note_index', note: note %>
        <% end %>
      <% else %>
        <p>いいねしたノートはありません</p>
      <% end %>
    </div>
  </div>
</div>