<div class="container">
  <div class="row">
    <div class="col-12">
      <H1>'<%= @tag.name %>'タグのノート一覧</h1><span>(全<%= @tag.notes.count %>件)</span>
      <% @tag_notes.each do |tag_note| %>
        <%= render '/public/shared/note_index', note: tag_note %>
      <% end %>
    </div>
  </div>
</div>